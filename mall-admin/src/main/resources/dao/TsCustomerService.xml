<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.macro.mall.dao.TsCustomerServiceDao">

    <select id="getByActivityId" resultMap="com.macro.mall.mapper.TsCustomerServiceMapper.BaseResultMap">
        SELECT *
	 FROM ts_customer_service as ts_customer_service where ts_customer_service.trs_activity_id = #{activityId}
    </select>
</mapper>