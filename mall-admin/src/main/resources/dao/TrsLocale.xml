<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.macro.mall.dao.TrsLocaleDao">

    <select id="getByActivityId" resultMap="com.macro.mall.mapper.TrsLocaleMapper.BaseResultMap">
        SELECT *
	 FROM trs_locale as trs_locale where trs_locale.trs_activity_id = #{activityId}
    </select>
</mapper>