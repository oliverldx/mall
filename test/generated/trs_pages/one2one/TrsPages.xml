<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.macro.mall.dao.TrsPagesDao">

    <select id="getByActivityId" resultMap="com.macro.mall.mapper.TrsPagesMapper.BaseResultMap">
        SELECT *
	 FROM trs_pages as trs_pages where trs_pages.trs_activity_id = #{activityId}
    </select>
</mapper>