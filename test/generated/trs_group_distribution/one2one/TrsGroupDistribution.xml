<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.macro.mall.dao.TrsGroupDistributionDao">

    <select id="getByActivityId" resultMap="com.macro.mall.mapper.TrsGroupDistributionMapper.BaseResultMap">
        SELECT *
	 FROM trs_group_distribution as trs_group_distribution where trs_group_distribution.trs_activity_id = #{activityId}
    </select>
</mapper>